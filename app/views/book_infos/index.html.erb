<h1>Listing basic book information</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th></th>
  </tr>

<% @bookinfos.each do |bookinfo| %>
  <tr>
    <td><%= bookinfo.title %></td>
    <td><%= bookinfo.author %></td>
    <td><%= link_to 'Show', BookInfo.find(bookinfo.id) %></td>
  </tr>
<% end %>

</table>

<br />

