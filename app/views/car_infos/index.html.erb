<h1>Listing cars</h1>

<table>
  <tr>
    <th>Registration</th>
    <th>Owner</th>
    <th></th>
  </tr>

<% @carinfos.each do |carinfo| %>
  <tr>
    <td><%= carinfo.registration %></td>
    <td><%= carinfo.owner %></td>
    <td><%= link_to 'Show', Car.find(carinfo.id) %></td>
  </tr>
<% end %>
</table>

<br />

