<h1>Listing cars</h1>

<table>
  <tr>
    <th>Parking</th>
    <th>Registration</th>
    <th>Owner</th>
    <th>Abonament card</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.parking_id %></td>
    <td><%= car.registration %></td>
    <td><%= car.owner %></td>
    <td><%= car.abonament_card %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

